{
  "name": "@gravity-ui/chartkit",
  "version": "0.8.0",
  "description": "React component used to render charts based on any sources you need",
  "license": "MIT",
  "repository": "git@github.com:gravity-ui/ChartKit.git",
  "main": "build/index.js",
