    "@gravity-ui/uikit": "^3.0.2",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-knobs": "^6.4.0",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.5.9",
    "@types/jest": "^28.1.3",
    "@types/lodash": "^4.14.177",
