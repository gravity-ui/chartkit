  "scripts": {
    "test": "jest",
    "test:watch": "jest --watchAll",
    "clean": "gulp clean",
    "start": "start-storybook -p 7007",
    "build": "gulp",
    "lint:js": "eslint --quiet --ext .js,.jsx,.ts,.tsx .",
